package com.ectrip.hqyt.model.request;


/**
 * @author xielei
 *
 */
public class LegalPersonRequest {

	//对于交易结算系统用户名
	private String usid;//用户名
	private boolean isMerchant;   //标识是否是商家还是散客
	private boolean active; //状态
	private Long id;		//商户ID
	
	
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getUsid() {
		return usid;
	}

	public void setUsid(String usid) {
		this.usid = usid;
	}

	public boolean isActive() {
		return active;
	}

	public void setActive(boolean active) {
		this.active = active;
	}

	public boolean isMerchant() {
		return isMerchant;
	}

	public void setMerchant(boolean isMerchant) {
		this.isMerchant = isMerchant;
	}
}
