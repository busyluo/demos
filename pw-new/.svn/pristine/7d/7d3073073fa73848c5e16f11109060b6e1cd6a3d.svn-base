package com.ectrip.ec.book.ticket.model;

import java.util.List;
import java.util.Map;

public class ProviderArea {
	
	private int szregionalid;
	private String szregionalname;
	private List<HotProvider> providerlist;
	private Map<String,List<HotProvider>> hotelmap;
	public int getSzregionalid() {
		return szregionalid;
	}
	public void setSzregionalid(int szregionalid) {
		this.szregionalid = szregionalid;
	}
	public String getSzregionalname() {
		return szregionalname;
	}
	public void setSzregionalname(String szregionalname) {
		this.szregionalname = szregionalname;
	}
	public List<HotProvider> getProviderlist() {
		return providerlist;
	}
	public void setProviderlist(List<HotProvider> providerlist) {
		this.providerlist = providerlist;
	}
	public Map<String, List<HotProvider>> getHotelmap() {
		return hotelmap;
	}
	public void setHotelmap(Map<String, List<HotProvider>> hotelmap) {
		this.hotelmap = hotelmap;
	}

}
