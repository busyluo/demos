package com.ectrip.sys.android.model;

public class MapPointBean {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * ,mapid      景点ID
	 * ,mapname    景点名称
	 * ,mapx       图上x坐标
	 * ,mapy,      图上Y坐标
	 * ,maplat     实际纬度
	 * ,maplng,    实际经度
	 * ,mapvideo,  语音解说词的路径（如：http://www.baidu.com/xxx.mp3）
	 * ,mapdesc,   景点简介
	 * ,maptype    类型（1景区，2为酒店）
	 */
	private String mapid,mapname,mapx,mapy,maplat,maplng,mapvideo,mapdesc,maptype;

	public String getMapid() {
		return mapid;
	}

	public void setMapid(String mapid) {
		this.mapid = mapid;
	}

	public String getMapname() {
		return mapname;
	}

	public void setMapname(String mapname) {
		this.mapname = mapname;
	}

	public String getMapx() {
		return mapx;
	}

	public void setMapx(String mapx) {
		this.mapx = mapx;
	}

	public String getMapy() {
		return mapy;
	}

	public void setMapy(String mapy) {
		this.mapy = mapy;
	}

	public String getMaplat() {
		return maplat;
	}

	public void setMaplat(String maplat) {
		this.maplat = maplat;
	}

	public String getMaplng() {
		return maplng;
	}

	public void setMaplng(String maplng) {
		this.maplng = maplng;
	}

	public String getMapvideo() {
		return mapvideo;
	}

	public void setMapvideo(String mapvideo) {
		this.mapvideo = mapvideo;
	}

	public String getMapdesc() {
		return mapdesc;
	}

	public void setMapdesc(String mapdesc) {
		this.mapdesc = mapdesc;
	}

	public String getMaptype() {
		return maptype;
	}

	public void setMaptype(String maptype) {
		this.maptype = maptype;
	}
}
